<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.us.mapper.NoticeMapper">
	<select id="list" resultType="org.us.model.NoticeVO">
		select * from notice
	</select>
	<update id="cntup">
		update notice
		set cnt = cnt + 1
		where nno = #{nno};
	</update>
</mapper>